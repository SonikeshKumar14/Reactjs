import React, { useState} from "react";
import ChildC from './ChildC'
 

function App() {
  const [add, setAdd] = useState(0);
  const Learning = () => {
    // some operation
  }
  return (
    <div className="App">
       <ChildC Learning={Learning}/>
       <h1>{add}</h1>
       <button onClick={()=> setAdd(add + 1)}>Add</button>
    </div>
  );
}
export default App;

